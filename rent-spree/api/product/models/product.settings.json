{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "decimal"
    },
    "imageUrl": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "stock": {
      "type": "integer",
      "default": 10
    },
    "enable": {
      "type": "boolean",
      "default": true
    },
    "quantity": {
      "type": "integer",
      "default": 0
    }
  }
}
